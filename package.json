{
  "name": "simple-scss-library",
  "version": "1.2.0",
  "description": "A simples CSS library.",
  "main": "dist/main.css",
  "scripts": {
    "build": "sass src/main.scss dist/main.css && postcss dist/main.css -o dist/main.css",
    "watch": "sass --watch src/main.scss dist/main.css",
    "start": "npm run build && npx serve"
  },
  "keywords": [
    "scss",
    "css",
    "library"
  ],
  "author": "Bruna Boeger",
  "license": "MIT",
  "readme": "README.md",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.0",
    "sass": "^1.83.4"
  }
}
